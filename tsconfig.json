{
  "compilerOptions": {
    "target": "es2017",
    "lib": ["es5", "es6", "es2015", "es2017", "es2021"],
    "outDir": "./dist",
    "module": "commonjs",
    "moduleResolution": "node",
    "sourceMap": true,
    "strict": true,
    "noImplicitAny": false,
    "useUnknownInCatchVariables": false,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true
  },
}
